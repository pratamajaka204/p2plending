
<div class="container">
	<div class="row">
		<h2>Selamat Datang</h2>
	</div>
	<div>
		<h3>USER</h3>
		 <div style="float:right"> <%= link_to 'Tambah Data', new_user_path %>
		</div>
		<% @users.each do |user| %>
		<table class="table table-hover table-responsive">
			<tr>
				<td><%= user.id %></td>
				<td><%= user.name %></td>
				<td><%= link_to 'detail', user_path(user) %></td>
			</tr>
		<% end %>
		</table>
	</div>
</div>

