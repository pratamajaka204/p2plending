<!DOCTYPE html>
<html>
<head>
  <title>P2plendingClon</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<%=  render 'layouts/navbar' %>
      <%= render 'layouts/breadcumb' %>
			<div class="row">
				<div class="col-md-3">

        <% if notice %>
      	    <%= notice %>
	      <% end %>

        <% if alert %>
      	    <%= alert %>
	      <% end %>

					<%= yield %>
				</div>
				<div class="col-md-9">
					
				</div>
			</div>
		<%= render 'layouts/footer' %>

	</div>
</body>
</html>
